<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <!--平移
    从（0，0）点移动到
    （300，300）点
    -->
    <translate
        android:fromXDelta="0"
        android:fromYDelta="300"
        android:toXDelta="0"
        android:toYDelta="0"
        android:duration="500"/>
</set>